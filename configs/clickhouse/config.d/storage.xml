<clickhouse>
  <storage_configuration>
    <disks>
      <minio>
        <type>s3</type>
        <endpoint>http://minio:9001/root/data/</endpoint>
        <access_key_id>minio</access_key_id>
        <secret_access_key>minio123</secret_access_key>
      </minio>
      <s3_cache>
        <type>cache</type>
        <disk>minio</disk>
        <path>/tmp/</path>
        <max_size>1Gi</max_size>
      </s3_cache>
    </disks>
    <policies>
      <external>
        <volumes>
          <s3>
            <disk>minio</disk>
          </s3>
        </volumes>
      </external>
    </policies>
  </storage_configuration>
</clickhouse>
